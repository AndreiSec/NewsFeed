{
  "expo": {
    "name": "NewsFeed",
    "slug": "NewsFeed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/splash/LOGO_N_IOSicon.png",
    "splash": {
      "image": "./src/assets/splash/LOGO_N_SPLASH.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF8745"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash/LOGO_N_ANDROID_ICON.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/logos/LOGO_N_300.png"
    }
  }
}
